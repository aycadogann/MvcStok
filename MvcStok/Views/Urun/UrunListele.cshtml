@using MvcStok.Models.Entity
@model List<Urunler>

@{
    ViewBag.Title = "UrunListele";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>ÜRÜN ID</th>
        <th>ÜRÜN ADI</th>
        <th>MARKASI</th>
        <th>KATEGORİ</th>
        <th>FİYAT</th>
        <th>STOK</th>
    </tr>

    <tbody>
        @foreach(var urun in Model)
            {
                <tr>
                    <td>@urun.UrunId</td>
                    <td>@urun.UrunAd</td>
                    <td>@urun.Marka</td>
                    <td>@urun.UrunKategori</td>
                    <td>@urun.UrunFiyat</td>
                    <td>@urun.Stok</td>
                </tr>
            }
    </tbody>

</table>

